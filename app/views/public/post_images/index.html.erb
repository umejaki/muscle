<div class="container">
  <div class="col">
    <table>
     <% @post_images.each do |post_image| %>
     <tbody>
      <tr>
       <td><%= @user.nick_name %></td>
       <td>
        <%= link_to post_image_path(post_image.id) do %>
        <%= post_image.title %>
        <% end %>
       </td>
       <td><%= post_image.introduction %></td>
      </tr>
     </tbody>
     <% end %>
    </table>
 </div>
</div>